<?xml version='1.0' encoding='utf-8'?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
<persistence-unit name="oop2020">
  <class>wsb.lasttask.ClassEntity</class>
  <class>wsb.lasttask.ClassSubjectTableEntity</class>
  <class>wsb.lasttask.StudentClassTableEntity</class>
  <class>wsb.lasttask.StudentsEntity</class>
  <class>wsb.lasttask.SubjectEntity</class>

  <properties>
    <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
    <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:5432/oop2020"/>
    <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQL94Dialect"/>
    <property name="hibernate.connection.username" value="postgres"/>
    <property name="hibernate.connection.password" value="passw0rd"/>
  </properties>
</persistence-unit>
</persistence>